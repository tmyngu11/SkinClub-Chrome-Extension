{
    "manifest_version": 3,
    "name": "Skin Club Helper",
    "description": "Calculates the profitability of cases",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "https://skin.club/*"
            ],
            "js": [
                "cases.js",
                "battles.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "webNavigation",
        "tabs"
    ]
}